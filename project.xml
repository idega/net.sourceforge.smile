<project>
    <pomVersion>3</pomVersion>
  	<extend>${base.project.dir}/base-project.xmlend>    
    <name>Smile</name>
    <groupId>bundles</groupId>
    <artifactId>net.sourceforge.smile</artifactId>
    <currentVersion>0.3</currentVersion>
    <organization/>
    <inceptionYear>2003</inceptionYear>
    <shortDescription>Smile - JSF Implementation</shortDescription>
    <description>Smile is an implementation of JavaServer Faces</description>
    <issueTrackingUrl>http://issues.idega.is</issueTrackingUrl>
    <versions/>
    <branches/>
    <developers>
        <developer>
            <name>Tryggvi Larusson</name>
            <id>tryggvil</id>
            <email>tryggvi@idega.is</email>
            <organization>idega software</organization>
            <roles/>
        </developer>
    </developers>
    <dependencies>
        <dependency>
            <groupId>smile</groupId>
            <artifactId>smile</artifactId>
            <version>0.3.2</version>
            <type>jar</type>
            <url>http://smile.sourceforge.net/</url>
            <properties>
            		<idegaweb.bundle.include>true</idegaweb.bundle.include>
            </properties>
        </dependency>
        <dependency>
            <groupId>myfaces</groupId>
            <artifactId>myfaces</artifactId>
            <version>1.0.5</version>
            <type>jar</type>
            <url>http://www.myfaces.org/</url>
            <properties>
            		<idegaweb.bundle.include>true</idegaweb.bundle.include>
            </properties>
        </dependency>        
        <dependency>
            <groupId>jdom</groupId>
            <artifactId>jdom</artifactId>
            <version>b9-rc2</version>
            <type>jar</type>
            <url>http://www.jdom.org/</url>
            <properties>
            		<idegaweb.bundle.include>true</idegaweb.bundle.include>
            </properties>
        </dependency>
        <dependency>
            <groupId>servletapi</groupId>
            <artifactId>servletapi</artifactId>
            <version>2.3</version>
            <type>jar</type>
            <url>http://java.sun.com/products/servlet/docs.html</url>
        </dependency>
        <dependency>
            <groupId>cos</groupId>
            <artifactId>cos</artifactId>
            <version>05Nov2002</version>
            <type>jar</type>
            <url>http://www.servlets.com/cos/</url>
            <properties>
            		<idegaweb.bundle.include>true</idegaweb.bundle.include>
            </properties>
        </dependency>
        <dependency>
            <groupId>jsf</groupId>
            <artifactId>jsf-api</artifactId>
            <version>1.0</version>
            <type>jar</type>
            <url>http://java.sun.com/j2ee/javaserverfaces/</url>
            <properties>
            		<idegaweb.bundle.include>true</idegaweb.bundle.include>
            </properties>
        </dependency>        
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
    </build>
    <properties/>
</project>